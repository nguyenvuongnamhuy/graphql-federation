    "test-doc-queue-proto:gen": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. apps/docs/src/proto/*.proto --ts_proto_opt=nestJs=true --ts_proto_opt=addGrpcMetadata=true || protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=. apps/docs/src/proto/*.proto --ts_proto_opt=nestJs=true --ts_proto_opt=addGrpcMetadata=true"
  },
