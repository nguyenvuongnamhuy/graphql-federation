  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"./**/*.{js,jsx,mjs,cjs,ts,tsx,json}\"",
    "start": "chmod +x start.sh & ./start.sh",
    "start:dev": "chmod +x start-dev.sh & ./start-dev.sh",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/graphql-gateway/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:ci": "eslint-output \"{src,apps,libs,test}/**/*.ts\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run db:apps/graphql-gateway/test:restart && npm run kafka:restart && dotenv -e .env.test -- jest --config ./test/jest-e2e.json",
    "test:ci": "jest --coverage --runInBand --ci --reporters=default --reporters=jest-junit --reporters=github-actions --json --coverage --testLocationInResults --outputFile=report/coverage/jest.json --verbose",
    "redis:up": "docker compose up redis -d",
    "redis:rm": "docker compose rm redis -s -f -v",
    "redis:restart": "npm run redis:rm && npm run redis:up",
    "prepare-dev": "husky install",
    "doc-queue-proto:gen": "protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. src/docs/queue/proto/*.proto --ts_proto_opt=nestJs=true --ts_proto_opt=addGrpcMetadata=true || protoc --plugin=protoc-gen-ts_proto=.\\node_modules\\.bin\\protoc-gen-ts_proto.cmd --ts_proto_out=. src/docs/queue/proto/*.proto --ts_proto_opt=nestJs=true --ts_proto_opt=addGrpcMetadata=true",
